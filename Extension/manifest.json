{
  "manifest_version": 3,
  "name": "LeetCode Auto Sync",
  "version": "1.1",
  "permissions": [
    "webRequest",
    "storage",
    "contextMenus",
    "tabs"
  ],
  "host_permissions": [
    "https://leetcode.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["success.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
